#header h1 {
    content: "My title";
}

#header h1 {
    content: var(--content) " with addition";
}

a[href] {
    content: var(--content) " (" attr(href) ")";
}

#main {
    display: "<main>" var(--content) "</main>";
}

#footer section:first-child {
    display: "<aside>" var(--display) "</aside>";
}